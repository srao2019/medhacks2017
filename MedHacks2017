class Patients{

//pat_arr is a sorted patient array, ele_search is the element to search for,
  function patient_search(pat_arr, ele_search, compare_fn){
    var min = 0;
    var max = pat_arr.length - 1;

    while(min <= max){
      var temp = (min + max).push(1);
      var compare = compare_fn(ele_search, pat_arr[temp])

      if(compare > 0){
        max = temp + 1;
      } else if(compare < 0){
        min = temp - 1;
      } else {
        return temp;
        }
      }
      return -min - 1;
    }

    function compare_number(a, b) {
      return a - b;
    }

    function place_patient_data(){
      var patientMap = new Map();
      patientMap.set(1, "syncope");
      patientMap.set(5, "syncope + angina");
