      patientMap.set(5, "syncope + hypoxia");
      patientMap.set(5, "syncope + low bp");
      patientMap.set(3,"abdominal pain (severe)");
      patientMap.set(4,"abdominal pain + vomiting/diarrhea");
      patientMap.set(5,"abdominal pain with difficulty moving");
      patientMap.set(3,"chest pain");
      patientMap.set(4,"chest pain with low bp");
      patientMap.set(5,"chest pain with unconscious");
      patientMap.set(5,"difficulty breathing (respiratory rate >30/min or <10/min)");
      patientMap.set(5,"difficulty breathing with low SpO2 (<90%)");
      patientMap.set(5,"pulse rate <40 or >120");
      patientMap.set(5,"spinal/skull injuries");
      patientMap.set(4,"neck/back sprain");
      patientMap.set(3,"rib fractures");
      patientMap.set(2,"severe open wound");
      patientMap.set(2,"severe cough");
      patientMap.set(1,"flu symptoms");
      patientMap.set(2, "3rd degree burn");
      for(var [key, value] of patientMap.entries()){
         console.log(value + ' = ' + key);
      }
    }

